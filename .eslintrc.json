{
  "$schema": "https://json.schemastore.org/eslintrc",
  "root": true,
  "plugins": ["prettier"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "@next/next/no-html-link-for-pages": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "@typescript-eslint/parser"
    }
  ]
}
